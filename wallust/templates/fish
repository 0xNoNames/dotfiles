set -U fish_color_normal normal
set -U fish_color_command {{color3|strip}}
set -U fish_color_param {{color11|strip}}
set -U fish_color_redirection $fish_color_param
set -U fish_color_comment {{color2|strip}}
set -U fish_color_error ff0000
set -U fish_color_escape {{color1|strip}}
set -U fish_color_operator $fish_color_escape
set -U fish_color_end {{color1|strip}}
set -U fish_color_quote {{color2|strip}}
set -U fish_color_autosuggestion {{color8|strip}}
set -U fish_color_user {{color12|strip}}
set -U fish_color_host {{color4|strip}}
set -U fish_color_valid_path --underline
set -U fish_color_cwd {{color4|strip}}
set -U fish_color_cwd_root red
set -U fish_color_match --background=brblue
set -U fish_color_search_match bryellow --background=brblack
set -U fish_color_selection white --bold --background=brblack
set -U fish_color_cancel -r
set -U fish_pager_color_prefix white --bold --underline
set -U fish_pager_color_completion {{color15 | strip}} --background={{color0 | strip}}
set -U fish_pager_color_description $fish_color_quote yellow
